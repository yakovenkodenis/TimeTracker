<!DOCTYPE html>
<html>
<head>
  <title>Time Tracker</title>
  <%= stylesheet_link_tag    'application' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>

	<%= render 'layouts/header' %>

	<div class="container">
		<% flash.each do |name, msg| %>
			<%= content_tag :div, msg, :id => "flash_#{name}" %>
		<% end %>

		<div class="row">
			<%= yield %>
		</div>
	</div>

</body>
</html>
